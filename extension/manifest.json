{
  "name": "Go Playground Addons",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Go Playground Addons",
  "icons": {
    "16": "icons/go-icon-16.png",
    "48": "icons/go-icon-48.png",
    "128": "icons/go-icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://play.golang.org/*"],
      "js": ["bundle.js"],
      "css": ["addon.css"]
    }
  ]
}
